<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ionicframework.redantagent828857" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <icon density="ldpi" src="www/img/logoRed.png" />
    <allow-intent href="market:*" />
    <name>Agent</name>
    <description>
        An Ionic Framework and Cordova project.
    </description>
    <author email="kimsonjulius1@gmail.com" href="http://example.com.com/">
      Julius Karanja.
    </author>
    <content src="index.html" />
    <access origin="*" />
    <access launch-external="yes" origin="tel:*" />
    <allow-intent href="tel:*" />
    <plugin name="cordova-plugin-geolocation" />
    <allow-intent href="geo:*" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="webviewbounce" value="false" />
    <preference name="UIWebViewBounce" value="false" />
    <preference name="Orientation" value="portrait" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="FadeSplashScreenDuration" value="2000" />
    <preference name="android-minSdkVersion" value="16" />
    <preference name="BackupWebStorage" value="none" />
</widget>
